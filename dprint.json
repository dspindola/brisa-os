{
  "typescript": {
    "forOfStatement.bracePosition": "nextLine",
    "forInStatement.bracePosition": "nextLine",
    "module.sortExportDeclarations": "caseInsensitive",
    "importDeclaration.forceMultiLine": "always",
    "exportDeclaration.forceMultiLine": "always",
    "lineWidth": 110,
    "indentWidth": 2
  },
  "json": {},
  "markdown": {
    "textWrap": "always",
    "lineWidth": 80,
    "emphasisKind": "underscores",
    "strongKind": "asterisks"
  },
  "toml": {
    "cargo.applyConventions": true
  },
  "dockerfile": {
    "lineWidth": 80
  },
  "biome": {
    "lineWidth": 110,
    "indentWidth": 2
  },
  "malva": {},
  "markup": {},
  "yaml": {
    "lineWidth": 110,
    "indentWidth": 2,
    "flowMap.preferSingleLine": false,
    "printWidth": 80
  },
  "excludes": ["**/node_modules", "**/*-lock.json"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.94.0.wasm",
    "https://plugins.dprint.dev/json-0.20.0.wasm",
    "https://plugins.dprint.dev/markdown-0.18.0.wasm",
    "https://plugins.dprint.dev/toml-0.7.0.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.2.wasm",
    "https://plugins.dprint.dev/biome-0.7.1.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.11.2.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.19.0.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.0.wasm",
    "https://plugins.dprint.dev/exec-0.5.1.json@492414e39dea4dccc07b4af796d2f4efdb89e84bae2bd4e1e924c0cc050855bf",
    "https://plugins.dprint.dev/prettier-0.57.0.json@1bc6b449e982d5b91a25a7c59894102d40c5748651a08a095fb3926e64d55a31"
  ]
}
